#include <stdio.h>
int main ()
{
    int chislo[9], a = 0, b = 0, c = 0;
    for(int i = 0;i < 9; i++ )
    {
        printf("vuvedi chislo:");
        scanf("%d", &chislo[i]);
    }
    while (a < 9)
    {
        while(b < 9)
        {
            if (chislo[b] > chislo[a])
            {
                c++;
            }
            b += 1;
        }
        if (c >= 2)
        {
            printf("chislo:%d \n", chislo[a]);
        }
        c = 0;
        b = 0;
        a += 1;
    }
}
